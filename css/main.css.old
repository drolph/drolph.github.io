/*
 * Defaults
 */

html, body {
	padding: 0;
	margin: 0;
}

html {
	font-size: 62.5%;
}

body {
	font-family: 'Lato', sans-serif;
	font-weight: 400;
	font-size: 1.2em;
	line-height: 1.4em;
	background-color: #272123;
	color: #222;
}

a {
	color: #222;
	text-decoration: none;
}

p a {
	padding-bottom: 1px;
	border-bottom: 1px solid #E5E5E5;
}

p a:hover {
	border-bottom: none;
}

img {
	border: 0;
}

p {
	padding: 0;
	margin: 0px 0 1px 0;
	line-height: 1.4em;
}

h0 {
	font-family: 'Lato', sans-serif;
	font-weight: 900;
	text-transform: uppercase;
	font-size: 1.26em;
	letter-spacing: 0.18em;
	/*line-height: 1.4em;*/
}

h1, h2, h3 {
	padding: 0;
	margin: 0;
	text-transform: uppercase;
	font-weight: normal;
}

h1 {
	font-size: 2.4em;
	letter-spacing: 5px;
}

h2 {
	font-size: 1.2em;
	margin-bottom: 5px;
}

h3 {
	margin-bottom: 5px;
	font-size: 1em;
}

h4, h5, h6 {
	font-family: 'Lato', sans-serif;
	text-transform: uppercase;
	text-align: right;
	padding: 0;
	/*margin: 0px 0 -10px 0;*/
}

h4 {
	color: #d9593d;
	font-weight: 400;
	font-size: 1.5em;
	/*letter-spacing: 0.2em;*/
	margin: 2px 0 0px 0;
}

h5 {
	font-weight: 900;
	font-size: 1.1em;
	margin: 0px 0 0px 0;
}

h6 {
	font-weight: 400;
	font-size: .8em;
	margin: -5px 0 0px 0;
}

h7 {
	font-weight: 900;
	font-size: 1.1em;
	margin: -10px 0 -20px 0;
}

/*
 * Utilities
 */

.align-left {
	float: left;
}

.align-right {
	float: right;
}

.align-center {
	margin: 0 auto 0 auto;
}

.column {
	float: left;
	margin-right: 10px;
	width: 300px;
}

.column.last {
	margin: 0;
}

/*
 * Parts
 */

#wrapper {
	margin: 0px auto 0px auto;
	/*margin: 50px auto 40px auto;*/
	width: 1000px;
}

#header {
	position: relative;
	height: 60px;
	margin-bottom: 15px;
	display: block;
}

#header #title {
	position: relative;
	float: left;
	height: 60px; 
	width: 160px;
	background-color: #d9593d;
	display: block;
}

#header #title h0 {
	width: 100%;
	position: absolute;
    bottom: 5px;
    text-align: center;
    color: #dce3e0;
	/*margin-top: 30px;*/
	display: inline;
}

/*
 * Menu
 */

#menu {
	padding: 40px 0 5px 0;
	position: absolute;
	width: 745px;
	height: 50px;
	/*bottom: 5px;*/
	text-align: center;
	text-transform: uppercase;
	display: inline;
	/*color: fff;*/
	text-decoration: none;
}

#menu a {
	color: #dce3e0;
}

#menu ul, #menu li {
	margin: 0;
	padding: 0;
	list-style: none;
}

#menu ul {
	width: 100%;
}

#menu li {
	display: inline;
	margin-right: 40px;
}

#menu li:last-child {
	margin-right: 0;
}

#menu li a {
	padding-bottom: 1px;
}

#menu li a:hover {
	border-bottom: 1px solid #b0c5bb;
}

#menu .active a {
	color: #d9593d;
}

/*#menu .active a:hover {
	color: #FF3366;
}*/

/*
* Icons
*/

#header #icons {
	position: relative;
	margin-top: 37px;
	float: right;
	display: block;
}

#header #icons img {
	padding: 0 0 0 5px;
}

/*
 * Parts
 */

#content {
	float: left;
	clear: both;
	padding: 0px 0 20px 0;
	width: 100%;
}

#subtitle {
	font-style: italic;
	font-size: 1.2em;
	text-align: center;
	padding: 20px;
	border-bottom: 1px #CCC solid;
}

#footbord {
	width: 100%;
	background-color: #dce3e0;
	overflow: auto;
	margin-bottom: 10px;
}

#footer {
	float: left;
	clear:both;
	/*height: 60px;*/
	/*margin-bottom: 10px;*/
	padding: 15px 0px 15px 20px;
	/*border-top: 1px #CCC solid;*/
	color: #000;
	display: block;
}


#powered-by {
	float: left;
	clear: both;
	margin-bottom: 40px;
	padding-top: 10px;
	width: 100%;
	border-top: 1px #CCC solid;
}

.page-text {
	width: 950px;
	margin: 0 15px 0 15px;
	
}


