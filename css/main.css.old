.project-info {
	margin-bottom: 10px;
	background-color: #dce3e0;
	padding: 10px 25px;
	overflow: auto;
}

.project-info h2 {
	margin-bottom: 0;
}

.tags {
	margin-bottom: 5px;
	width: 240px;
}

.tags, .tags a {
	color: #999;
}

.tags a:hover {
	color: #EF2400;
}

.fileGroup {
	float: left;
	margin: 0 10px 10px 0;
	width: 950px;
}

.file {
	float: left;
}

.file img {
	/*display: block;*/
	z-index:-1;
	margin-bottom: 10px;
	max-width: 950px;
	height: auto;
}

.textblock {
	display: block;
	padding: 0 auto 0 auto;
	float: left;
	margin: 0 20px 20px 0;
	width: 425px;
}

.slideshow {
	/*display: block;*/
	/*position: relative;*/
	margin-bottom: 1px;
	max-width: 950px;
	height: auto;
}

.slideshow .next {
	background-color: #272123;
	/*display: block;*/
	opacity: 1;
    position: absolute;
    z-index:5;
    /*width: 50%;*/
    height: 13px;
    top: 80%;
    left: 80%;
}

.slideshow .slideshow-nav {
	font-size: .8em;
	margin-bottom: 3px;
}

.slideshow-nav {
	position: absolute;
	z-index: 15;
}

.slideshow .slideshow-nav a, .file .info .title {
	/*color: #EF2400;*/
}

.slideshow .slideshow-nav, .file .info .caption {
	color: #999;
}

.slideshow .slideshow-nav .currentSlide {
	font-weight: 900;
}

/*
 * Blog
 */

.post {
	margin: 0px 0 40px 0;
	width: 500px;
}

.postTitle {
	margin-bottom: 0;
}

.postTitle a:hover {
	text-decoration: underline;
}

.post img {
	display: block;
	margin: 0 0 10px 0;
	padding: 0;
}

.post .text {
	margin: 20px 0 0 0;
}

.post .text a{
	color:000;
	text-decoration: none;
}

.post .date {
	color: #999;
}

.post .more {
	margin-left: 5px;
	font-size: 1em;
	color: #EF2400;
}

.post .more:hover {
	color: #999;
}

.back {
	color: #EF2400;
}

.back:hover {
	text-decoration: underline;
}

#footer .post {
	margin: 0;
	width: 100%;
}

#footer .postTitle {
	margin-bottom: 0;
	font-weight: bold;
	color: #EF2400;
}

#footer .date {
	display: block;
	clear: both;
}

#footer .text {
	margin-top: 10px;
	width: 100%;
}

#footer .more {
	font-size: 1.2em;
}